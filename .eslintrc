{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "import"
    ],
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    },
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "indent": ["error", 4],
      "comma-dangle": ["error", "never"]
    }
}